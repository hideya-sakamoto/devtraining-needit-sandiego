        <order/>
        <script><![CDATA[function onLoad() {
   //Type appropriate comment here, and begin script below
	var var_needit_users_grp = 'NeedIt Users';
	var var_state = g_form.getValue('state');
	
	//ステータスが完了かつNeedIt Usersに所属するユーザでログインした場合
	if(var_state == 'Closed Complete' && g_user.hasRole(var_needit_users_grp) ){
